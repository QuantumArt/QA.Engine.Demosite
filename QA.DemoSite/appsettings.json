{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  },
  "ConnectionStrings": {
    "DatabaseQP": "Application Name=DemoSite;Initial Catalog=__db.name__;Data Source=__db.server__;User ID=__db.user__;Password=__db.password__;MultipleActiveResultSets=True;",
    "DatabaseQPPostgre": "Server=__postgre.db.server__;Database=__postgre.db.name__;User Id=__postgre.db.user__;Password=__postgre.db.password__"
  },
  "QpSettings": {
    "SiteId": 52,
    "IsStage": "__settings.isStage__",
    "DatabaseType": "__qp.databasetype__",
    "CustomerCode": "__qp.customercode__"
  },

  "QpSiteStructureSettings": {
    "CachePeriod": "00:01:00",
    "RootPageDiscriminator": "root_page",
    "UploadUrlPlaceholder": "<%=upload_url%>",
    "LoadM2mForAbstractItem": true,
    "LoadAbstractItemFieldsToDetailsCollection": true
  },

  "OnScreen": {
    "AdminSiteBaseUrl": "__settings.onscreenUrl__"
  }

}
